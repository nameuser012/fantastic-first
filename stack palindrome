#include <stdio.h>
#pragma warning(disable:4996)
#include <string.h>
#include <stdlib.h>
#define SIZE 100

typedef struct StackType {
    char arr[100];
    int top;
}StackType;

//초기화
void init(StackType* s) {
    s->top = -1;
}

// 스택이 비어있으면 리턴 1 아니면 리턴 0
int is_empty(StackType* s) {
    if (s->top == -1)
        return 1;
    return 0;
}

// 스택이 가득 차있으면 리턴 1 아니면 리턴 0
int is_full(StackType *s) {
    if (s->top == SIZE - 1)
        return 1;
    return 0;
}

void push(StackType* s, char value) {
    if (is_full(s)) {
        printf("Stack is full");
        exit(1);
    }
    s->arr[++(s->top)] = value;
}

int pop(StackType* s) {
    if (is_empty(s)) {
        printf("Stack is empty");
        exit(1);
    }
    return s->arr[(s->top)--];
}

int peek(StackType* s) {
    if (is_empty(s)) {
        printf("Stack is empty");
        exit(1);
    }
    return s->arr[s->top];
}

int main() {
    StackType s;
    init(&s);
    char c;
    scanf("%c", &c);
    while (c != '\n') {
        if (c != ' ')
            push(&s, c);
        scanf("%c", &c);
    }
    int n = s.top;
    int i;
    for (i = 0; i <= n/2 && (s.arr[i] == s.arr[s.top] || s.arr[i]+32 == s.arr[s.top] || s.arr[i] == s.arr[s.top] + 32); i++) {
        pop(&s);
    }
    if (i == n / 2 + 1)
        printf("TRUE");
    else
        printf("FALSE");
    
    return 0;
}
